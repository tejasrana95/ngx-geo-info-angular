{
  "name": "ngx-geo-info-angular",
  "version": "1.0.0",
  "description": "A module which gives you GEO info of user 'latitude', 'longitude', 'accuracy', 'altitude', 'altitudeAccuracy', 'heading', 'speed', 'timestamp' and best of all it is AOT ready",
  "main": "dist/bundles/ngx-geo-info-angular.umd.js",
  "scripts": {
    "cleanup": "rimraf dist/bundles dist/src dist/index.d.ts dist/index.js dist/index.js.map dist/LICENCE dist/README.md",
    "bundling": "rollup -c",
    "minify": "uglifyjs dist/bundles/ngx-geo-info-angular.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/ngx-geo-info-angular.umd.min.js",
    "build": "npm run cleanup && ngc && npm run bundling && npm run minify"
  },
  "author": {
    "name": "Tejas Rana",
    "email": "me@tejasrana.com"
  },
  "bugs": {
    "url": "https://github.com/tejasrana95/ngx-geo-info-angular/issues",
    "email": "me@tejasrana.com"
  },
  "contributors": [
    {
      "name": "Tejas Rana",
      "email": "me@tejasrana.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tejasrana95/ngx-geo-info-angular.git"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/core": "^2.4.0",
    "reflect-metadata": "^0.1.8",
    "rxjs": "^5.0.1",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "@angular/compiler": "^2.4.0",
    "@angular/compiler-cli": "^2.4.0",
    "rimraf": "^2.5.4",
    "rollup": "^0.37.0",
    "typescript": "~2.0.10",
    "uglify-js": "^2.7.5"
  }
}